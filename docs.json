{
  "openapi": "3.0.0",
  "host": "https://docs.crustplay.com",
  "info": {
    "title": "FanCrew - API",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://fantasy.crustplay.com",
      "description": "Development Server"
    }
  ],
  "apis": [
    "Routes/*.js",
    "Models/*.js",
    "Controllers/*.js",
    "Services/*.js"
  ],
  "paths": {
    "/user": {
      "get": {
        "description": "Get Current User",
        "responses": {
          "200": {
            "description": "ok",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            }
          }
        }
      }
    },
    "/user/:id": {
      "get": {
        "description": "Get Single User",
        "response": {
          "200": {
            "description": "ok",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "User": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string"
          },
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "username": {
            "type": "string"
          }
        }
      }
    }
  },
  "tags": []
}